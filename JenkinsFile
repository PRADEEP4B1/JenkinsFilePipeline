    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
             stage('Build') {
            steps {
                echo 'Building Hello World'
            }
             }
             
        
         stage('Deploy') {
            steps {
                echo 'Deploying'
            }
         }
            
             stage('Test') {
            steps {
                echo 'TestingHello World'
            }
             }
             
        
         stage('Release') {
            steps {
                echo 'Release Hello World'
            }
        }
        
        
        }
    
    
